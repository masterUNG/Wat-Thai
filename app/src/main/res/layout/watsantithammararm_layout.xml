<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	tools:context="com.example.manit.appwatthai.indexactivity.page.WatsantithammararmActivity"
	android:orientation="vertical">
	<RelativeLayout
		android:id="@+id/relativeLayout1"
		android:layout_width="fill_parent"
		android:layout_height="40dp"
		android:background="#d2be85" >
		<ImageButton
			android:id="@+id/back"
			android:layout_width="40dp"
			android:layout_height="40dp"
			android:background="@android:drawable/ic_menu_revert" />
		<TextView
			android:id="@+id/textView1"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_centerHorizontal="true"
			android:layout_centerVertical="true"
			android:text="WATTHAI"
			android:textColor="@color/black"
			android:textSize="22dp"
			android:textStyle="bold"
			android:typeface="sans" />
		<ImageButton
			android:id="@+id/btn_location13"
			android:layout_alignParentRight="true"
			android:layout_width="40dp"
			android:layout_height="40dp"
			android:background="@android:drawable/ic_menu_mapmode" />
	</RelativeLayout>
	<ScrollView
		xmlns:android="http://schemas.android.com/apk/res/android"
		android:fitsSystemWindows="true"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:id="@+id/scrollView">
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:orientation="vertical">

			<WebView
				xmlns:android="http://schemas.android.com/apk/res/android"
				android:id="@+id/webViewn13"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				/>


		</LinearLayout>
	</ScrollView>
	</LinearLayout>